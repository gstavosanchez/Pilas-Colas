<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="536.0" style="-fx-background-color: #DFDFDF;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.gustavosanchez.controlador.ClienteProductoController">
